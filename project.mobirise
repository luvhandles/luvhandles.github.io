                  "transform": "translate(0px, 20px)"
                },
                ".svg11": {
                  "transform": "skew(0deg, 10deg) scaleX(1) scaleY(0.9) rotate(-10deg)"
                },
                ".svg12": {
                  "transform": "skew(0deg, 10deg) scaleX(1) scaleY(0.9) rotate(30deg)"
